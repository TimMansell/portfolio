{
  "name": "tim",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "4.1.3",
    "classnames": "^2.2.5",
    "date-fns": "^2.0.0-alpha.1",
    "font-awesome": "^4.7.0",
    "in-viewport": "^3.5.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.9.0",
    "normalize-scss": "^7.0.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-scripts": "1.0.8",
    "react-scroll": "^1.5.3",
    "react-scrolllock": "^1.0.8",
    "redux": "^3.7.2",
    "serve": "^6.0.2",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "build:css": "node-sass --include-path ./node_modules src/scss -o src/css/",
    "watch:css": "npm run build:css && node-sass --include-path ./node_modules src/scss -o src/css/ --watch --recursive",
    "start": "run-p watch:css start:react",
    "build": "run-s build:css build:react",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "serve": "serve -s build"
  }
}
